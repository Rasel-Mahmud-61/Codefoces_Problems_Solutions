#include<bits/stdc++.h>
using namespace std;
int main()
{
    int  t;
    cin>>t;
    while(t--)
    {
        int n;
        cin>>n;
        if(n<4)
        {
            for(int i=1;i<=n;i++) cout<<i<<" ";
            cout<<endl;
        }
        else
        {
            int l=n/2+1;
            int r=1;
            while(l<=n)
            {
                cout<<l<<" ";
                l++;
                cout<<r<<" ";
                r++;
                // n odd number hole
                if(n==l && n%2==1)
                {
                    cout<<l<<endl;
                    break;
                }
            
            }
            cout<<endl;
        }

    }
}
