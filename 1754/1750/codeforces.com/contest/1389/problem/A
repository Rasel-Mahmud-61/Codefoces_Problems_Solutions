#include<bits/stdc++.h> 
using namespace std;  
#define ll long long 
int main()
{
   int t;
   cin>>t;
   while(t--)
   {
    ll l,r;
    cin>>l>>r;
     ll x=0,y=0;
     if(2*l<=r)
     {
        x=l;
        y=2*l;

     }
     else{
        x=-1;
        y=-1;

     
     
     }
     cout<<x<<" "<<y<<endl;

   }

}
