#include<bits/stdc++.h> 
using namespace std;
#define ll long long 
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        ll p,a,b,c;
        cin>>p>>a>>b>>c;
         ll x=a-p%a;
         ll y=b-p%b;
         ll z=c-p%c;
         if(p%a==0 || p%b==0 || p%c==0)
         {
            cout<<"0"<<endl;
         }
         else
         {
            cout<<min(x,min(y,z))<<endl;
         }
    }
}
