#include<bits/stdc++.h> 
using namespace std;
int main()
{
     int t;
     cin>>t;
     while(t--)
     {
        int n;
        cin>>n;
         string s;
         cin>>s;
         int ans=1;
         for(int i=1;i<n;i+=3)
         {
            if(s[i]!=s[i+1])
            {
                ans=0;
                break;
            }
         }
         if(ans==1) cout<<"YES"<<endl;
         else cout<<"NO"<<endl;
     }
}
