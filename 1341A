#include<bits/stdc++.h>
using namespace std;
int main()
{
     int t;
     cin>>t;
     while(t--)
     {
        int n,a,b,c,d;
        cin>>n>>a>>b>>c>>d;
        int min1=n*(a-b);
        int max1=n*(a+b);
       
      
       // cout<<min1<<" "<<max1<<endl;
       //  cout<<min2<<" "<<max2<<endl;
       
        if(min1>c+d || max1<c-d) cout<<"No"<<endl;
        else cout<<"Yes"<<endl;
     }
}
