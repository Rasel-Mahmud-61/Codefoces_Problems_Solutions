#include<bits/stdc++.h> 
using namespace  std;
#define ll long long 
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        ll n;
        cin>>n;
        ll ans=0;
        for(int i=1;i<=9;i++)
        {
            ll p=0;
            while(p<=n)
            {
                p=p*10+i;
                if(p<=n)
                {
                    ans++;
                }
            }
        }
        cout<<ans<<endl;
    }
return 0;
}
