#include<bits/stdc++.h> 
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        int k;
        cin>>k;
        int ans=0;
       int i=1;
       while(1)
       {
           
          int temp=i;
            int rem=temp%10;
            if(i%3!=0 && rem!=3)
            {
                ans=i;
                k--;
              //  cout<<k<<" "<<i<<endl;
                
                if(k==0) break;
            }
            i++;
       }
     cout<<ans<<endl;
    }
}
