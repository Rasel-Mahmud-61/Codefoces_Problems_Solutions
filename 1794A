#include<bits/stdc++.h> 
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
         int n;
         cin>>n;
        vector<string>str;
         for(int i=0;i<2*n-2;i++)
         {
            string s;
            cin>>s;
            if(s.size()==n-1)
            {
                str.push_back(s);
            }
           
         }
         reverse(str[0].begin(),str[0].end());
         if(str[0]==str[1])
         {
            cout<<"YES"<<endl;
         }
         else cout<<"NO"<<endl;
       
    }
}
