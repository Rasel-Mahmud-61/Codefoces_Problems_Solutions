#include<bits/stdc++.h>
using namespace std;
#define ll long long
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
         ll n;
         cin>>n;
         string s;
         cin>>s;
         for (int i=0;i<n;i++)
         {
             s[i]=tolower(s[i]);
         }
         string ans;
         for(int i=0;i<n;i++)
         {
             if(s[i]!=s[i+1])
                ans+=s[i];
         }
         //cout<<ans<<endl;
         if(ans=="meow") cout<<"YES"<<endl;
         else cout<<"NO"<<endl;
    }
}
