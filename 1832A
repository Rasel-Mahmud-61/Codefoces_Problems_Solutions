#include<bits/stdc++.h>
using namespace std;
int main()
{

   int t;
   cin>>t;
   while(t--)
   {
       string s;
       cin>>s ;
       bool same=true;
       for(int i=1;i<s.size()/2;i++)
       {
          if(s[i]!=s[0])
          {
              same=false;
              break;
          }
       }
       if(!same) cout<<"YES"<<endl;
       else cout<<"NO"<<endl;
   }

}
