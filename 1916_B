#include<bits/stdc++.h>

using namespace  std;
 #define ll long long int
 int main(){
    ll t;
    cin>> t;
    while(t--){

        ll a,b;
        cin>>a>>b;
         if(b%a==0)
         {
             cout<<(b*b)/a<<endl;

         }
         else{

            ll lcm=(a*b)/__gcd(a,b);


            cout<<lcm<<endl; 
         }
    }
 }
