#include<bits/stdc++.h>
using namespace std;


int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        int n;
        cin>>n;
        int ar[n];
        int b[n];

        for(int i=0;i<n;i++){
            cin>>ar[i];
            b[i]=n+1-ar[i];
        }
        for(int i=0;i<n;i++) cout<<b[i]<<" ";

        cout<<endl;

    }
}
